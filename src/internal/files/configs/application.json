{
	"gin": {
		"cors": {
			"allow_methods": ["GET", "POST", "PUT", "DELETE"],
			"allow_origins": ["*"]
		},
		"jwt": {
			"expiration": 180
		},
		"cache": {
			"provider": "memory",
			"redis": {
				"host": "cache",
				"port": 6379,
				"password": "",
				"db": 0
			}
		},
		"host": "0.0.0.0",
		"port": 8888
	},
	"container": {
		"provider": "docker",
		"entry": "127.0.0.1",
		"docker": {
			"uri": "unix:///var/run/docker.sock"
		},
		"k8s": {
			"namespace": "default",
			"config": {
				"path": "./configs/k8s.yml"
			}
		},
		"proxy": {
			"enabled": false,
			"traffic_capture": {
				"enabled": false
			}
		}
	},
	"db": {
		"provider": "postgres",
		"postgres": {
			"dbname": "cloudsdale",
			"host": "db",
			"username": "cloudsdale",
			"password": "cloudsdale",
			"port": 5432,
			"sslmode": "disable"
		},
		"mysql": {
			"dbname": "cloudsdale",
			"host": "db",
			"username": "cloudsdale",
			"password": "cloudsdale",
			"port": 3306
		},
		"sqlite": {
			"path": "./db/db.sqlite"
		}
	},
	"email": {
		"address": "",
		"password": "",
		"smtp": {
			"host": "",
			"port": 0
		}
	},
	"captcha": {
		"provider": "turnstile",
		"turnstile": {
			"url": "https://challenges.cloudflare.com/turnstile/v0/siteverify",
			"site_key": "",
			"secret_key": ""
		},
		"recaptcha": {
			"url:": "https://www.google.com/recaptcha/api/siteverify",
			"site_key": "",
			"secret_key": "",
			"threshold": 0.5
		}
	}
}
